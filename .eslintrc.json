{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "max-classes-per-file": "off",
    "object-curly-newline": "off",
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2021
  }
}